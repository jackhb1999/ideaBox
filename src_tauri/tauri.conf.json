{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "idea box",
  "version": "0.1.0",
  "identifier": "com.rustob",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Idea Box",
        "alwaysOnTop": true,
        "transparent": true,
        "width": 400,
        "height": 800,
        "minWidth": 400
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/Pills.png",
      "icons/ip.png",
      "icons/ip.png",
      "icons/idea.icns",
      "icons/idea.ico"
    ]
  }
}
